<!DOCTYPE html>
<html lang="zxx">

<head>

    <title>Game Warrior Template</title>
    <meta charset="UTF-8">
    <meta name="description" content="Game Warrior Template">
    <meta name="keywords" content="warrior, game, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script language="Javascript" type="text/javascript" src="/counter/js/jquery-1.4.1.js"></script>
    <script language="Javascript" type="text/javascript" src="/counter/js/jquery.lwtCountdown-1.0.js"></script>
    <script language="Javascript" type="text/javascript" src="/counter/js/misc.js"></script>


    <!-- Favicon -->
    <link href="/img/favicon.ico" rel="shortcut icon" />

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,400i,500,500i,700,700i" rel="stylesheet">

    <!-- Stylesheets -->
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/css/owl.carousel.css" />
    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/animate.css" />
    <link href="~/css/Wheel.css" rel="stylesheet" />
    <link href="~/css/canli.css" rel="stylesheet" />

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body style="margin-right:4px;">
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>
 
    <!-- Header section -->
    <header class="header-section" style="height:100px">
        <div class="container">
            <!-- logo -->
            <a class="site-logo" href="index.html">
                <p style="font-size:50px;font-family:none;margin-top:-30px"><span style="color:gold">Bill</span><span style="color:white">Bakalım</span></p>
            </a>
            @if (Request.IsAuthenticated)
            {
                <div class="user-panel">
                    <div class="site-btn">@Html.ActionLink("Hesabım", "AccountInfo", "Account")/@Html.ActionLink("Çıkış Yap", "LogOut", "Account")</div>
                </div>
            }
            <!-- responsive -->
            <div class="nav-switch">
            </div>
            <!-- site menu -->
            <nav class="main-menu" style="text-align:center">
                <ul>
                    <li>@Html.ActionLink("Anasayfa", "Index", "Home")</li>
                    <li>@Html.ActionLink("Oyun", "ContestBegin", "Home")</li>
                    <li>@Html.ActionLink("Yarışmalar", "ContestOfDate", "Home")</li>
                    <li>@Html.ActionLink("Hakkında", "About", "Home")</li>
                    <li>@Html.ActionLink("Destek", "CanliDestek", "Home")</li>
                </ul>
            </nav>
        </div>
    </header>
    <!-- Header section end -->
    <!-- Hero section -->
    <div> @RenderBody()</div>
    <div id="container">
        @*<div id="timer"></div>*@
        <span id="spin">
            <canvas id="canvas" width="500" height="500"></canvas>
        </span>
        <h2 id="category"></h2>
    </div>
    <!-- Footer top section end -->
    <!-- Footer section -->
    <footer class="footer-section">
        <div class="container">
            <ul class="footer-menu">
                <li><a href="index.html">Home</a></li>
                <li><a href="review.html">Games</a></li>
                <li><a href="categories.html">Blog</a></li>
                <li><a href="community.html">Forums</a></li>
                <li><a href="contact.html">Contact</a></li>
            </ul>
            <p class="copyright">
                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                Copyright &copy;
                <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
            </p>
        </div>
    </footer>
    <!-- Footer section end -->
    <!--====== Javascripts & Jquery ======-->
    <script src="/js/jquery-3.2.1.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/owl.carousel.min.js"></script>
    <script src="/js/jquery.marquee.min.js"></script>

    <script src="/js/main.js"></script>
    <script src="~/js/Wheel.js"></script>
    <script src="~/js/CanliDestek.js"></script>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery.signalR-2.4.0.min.js"></script>
    <script src="~/SignalR/Hubs"></script>
    @Scripts.Render("~/bundles/jquery-unobtrusive")


</body>
</html>